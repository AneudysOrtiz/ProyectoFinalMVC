            <Source Path="DBContext" DbContext="SistemaRH3.Models.ApplicationDbContext, SistemaRH3" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="SistemaRH3.DAL.GeneralContext" Order="2" Enabled="True">
          <Destination Path="Data Source=(LocalDB)\MSSQLLocalDB;Initial Catalog=SistemaRH3;Integrated Security=True" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="SistemaRH3.DAL.GeneralContext, SistemaRH3" MigrationConfiguration="SistemaRH3.Migrations.Configuration, SistemaRH3" Origin="" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="DataBase" Order="3" Enabled="False">
