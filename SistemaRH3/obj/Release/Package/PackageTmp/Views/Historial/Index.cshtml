@model IEnumerable<SistemaRH3.Models.Historial>

@{
    ViewBag.Title = "Index";
}

  <!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Historial <small>Detalles</small>
        </h1>
        <ol class="breadcrumb">
            <li class="active">
                <a class="btn btn-sm btn-default"
                   href="@Url.Action("Index", "Home")">
                    <span class="glyphicon glyphicon-arrow-left"></span> Volver
                </a>

                <a class="btn btn-sm btn-success"
                   href="#"><span class="glyphicon glyphicon-print"></span> Exportar PDF</a>
            </li>
        </ol>
    </div>
</div>


<table class="table table-hover">
    <thead>
        <tr>
            <th>Fecha</th>
            <th>Descripcion</th>

        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.fecha</td>
                <td>@item.descripcion @item.elementoNombre @item.elementoApellido</td>
            </tr>
        }
    </tbody>
</table>
